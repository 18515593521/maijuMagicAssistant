/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/*活动*/
.activity_box{
  width:750rpx;
  overflow:hidden;
  height:auto;
  background:#fff;
}
.activit_list{
    width:750rpx;
    height:auto;
    float:left;
    overflow:hidden;
    border-top:10px solid #eee;
}
.activity_img{
  width:750rpx;
  height:400rpx;
  float:left;
}
.activity_name{
  line-height:18px;
  padding:5rpx 20rpx;
  box-sizing:border-box;
  font-size:24rpx;
  color:#666;
  float:left;
}

/*超出去后隐藏*/
.hiddenLien{
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
/*产品*/
.list_all_show{
  width:750rpx;
  height: auto;
  display:flex;
  flex-flow:row wrap;
}

.list_item_box{
  margin:0 10rpx 10rpx;
  width:350rpx;
  border:2rpx solid #f2f3f3;
  height:auto;
  overflow:hidden;
  display:flex;
  flex-flow:column nowrap;
}
.list_img{
  width:100%;
  height:350rpx;
}
.list_name{
  color:#666;
  background:#f3f3f3;
  font-size:26rpx;
  padding:6rpx 20rpx;
  box-sizing:border-box;
}

.borderTop{
  border-top:2rpx solid #eee;
}
.botderBottom{
  border-bottom:2rpx solid #eee;
}
.paddingBox{
  padding:20rpx;
  box-sizing:border-box;
}
.flexRow{
  display:flex;
  flex-flow:row nowrap;
}
.flexRow2 {
    display:flex;
  flex-flow:row wrap;
}
.flexColumnH{
  display:flex;
  flex-flow:column nowrap;
}
.flexColumnNo{
  display:flex;
  flex-flow:column wrap;
}
.positionBox{
  position:fixed;
  margin:auto;
  top:0;
  right:0;
  bottom:0;
  left:0;
  background:rgba(0, 0, 0, 0.5);
  z-index:10000;
}
.searchImg{
  width:45rpx;
  height:45rpx;
  position:absolute;
  margin:auto;
  right:30rpx;
  top:28rpx;
  z-index:1000;
}
.justAround{
  justify-content:space-around;
}
.justBetween{
  justify-content:space-between;
}
/*超出去后隐藏*/
.hiddenLien{
  overflow:hidden;
