.integral_box{
  width:750rpx;
  height:360rpx;
  background:#4abdc2;
}
.score_title{
  color:#fff;
  font-size:26rpx;
  height:60rpx;
}
.score_code{
  width:100%;
  height:300rpx;
  line-height:300rpx;
  text-align:center;
  color:#fff;
  font-size:120rpx;
}
.score_box_list{
  width:750rpx;
  height:auto;
}
.images{
  width:50rpx;
  height:60rpx;
  display:flex;
}
.score_item{
  padding:0 20rpx;
  box-sizing:border-box;
  width:100%;
  height:100rpx;
  border-bottom:2rpx solid #eee;
  align-items:center;
}
.title{
  width:700rpx;
  font-size:28rpx;
  line-height:80rpx;
}
.images image{
  width:35rpx;
  height:35rpx;
  align-items:center;
  margin:auto;
}