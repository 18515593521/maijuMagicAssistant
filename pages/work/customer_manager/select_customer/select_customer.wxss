.search_box{
  height:100rpx;
  overflow:hidden;
  background:#fff;
}
.searchList{
  width:750rpx;
}
.search{
  width:100%;
  height:100%;
  position:relative;
}
.search_input{
  font-size:28rpx;
  border-radius:10rpx;
  border:2rpx solid #eee;
  width:100%;
  height:100%;
  color:gray;
  padding-right:40rpx;
  box-sizing:border-box;
  z-index:0;
  background:#f5f5f5;
}
.search image{
  width:45rpx;
  height:45rpx;
  position:absolute;
  margin:auto;
  right:8rpx;
  top:8rpx;
  z-index:10000;
}
.customerItems{
  padding:0 20rpx;
  box-sizing:border-box;
  width:750rpx;
    border-bottom:2rpx solid #eee;
}
.customerItems view{
  font-size:30rpx;
  line-height:80rpx;
  color:#333;

}
.submit{
  width:750rpx;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  color:#fff;
  font-size:30rpx;
  background:#4abdc2;
  position:fixed;
  bottom:0;
  left:0;
  right:0;
}
.radioSelect{
    transform:scale(0.6)
}